extends ../aalayout/layout

block content
	.container.mt-5
		.row.mt-5
			.col-12
				h3 #{title}
		.row.mt-5
			each nome in nomes
				-a;
				if nome.code && nome.code.length > 0
					-a;
					.col-md-4.col-lg-3.mt-3
						a.btn.btn-info(href="/products?keyword="+nome.code) #{nome.code}

	.js-browserHeight-autoFooter